{
	"name": "http-client",
	"displayName": "Http Client",
	"description": "postman like http client",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:http-client.OpenHttpClient"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "http-client.OpenHttpClient",
				"title": "Http Client: Open Http Client"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch:ext": "webpack --watch --config ./build/node-extension.webpack.config.js",
		"watch:view": "webpack --watch --mode development --config ./build/view.webpack.config.js",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts,js,tsx",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.3",
		"@types/react-router-dom": "^5.1.7",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"css-loader": "^5.2.2",
		"eslint": "^7.15.0",
		"eslint-config-prettier": "^8.2.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.23.2",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.2.1",
		"style-loader": "^2.0.0",
		"ts-loader": "^9.0.0",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.33.2",
		"webpack-cli": "^4.6.0"
	},
	"dependencies": {
		"antd": "^4.15.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.2.0"
	}
}
